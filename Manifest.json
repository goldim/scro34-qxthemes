{
  "$schema": "https://qooxdoo.org/schema/Manifest-1-0-0.json",
  "info": {
    "name": "scro34-qxthemes",
    "summary": "bunch of themes",
    "description": "a bunch of themes: Dark, SilverBlue, Gradient and etc.",
    "homepage": "https://github.com/goldim/scro34-themes",
    "license": "MIT license",
    "authors": [
      {
        "name": "Norbert Schr√∂der (scro34)",
        "email": "schroeder@scro34.de"
      }
    ],
    "version": "1.0.0"
  },
  "provides": {
    "namespace": "scro34.theme",
    "encoding": "utf-8",
    "class": "source/class",
    "resource": "source/resource",
    "translation": "source/translation",
    "application": {
      "class": "widgetbrowser.Application",
      "theme": "scro34.theme.Dark",
      "name": "widgetbrowser",
      "title": "WidgetBrowser",
      "include": [
        "scro34.theme.Dark",
        "scro34.theme.Graydient",
        "scro34.theme.SilverBlue",

        "widgetbrowser.pages.Tree",
        "widgetbrowser.pages.List",
        "widgetbrowser.pages.Table",
        "widgetbrowser.pages.Form",
        "widgetbrowser.pages.ToolBar",
        "widgetbrowser.pages.Window",
        "widgetbrowser.pages.Tab",
        "widgetbrowser.pages.Control",
        "widgetbrowser.pages.Embed",
        "widgetbrowser.pages.EmbedFrame",
        "widgetbrowser.pages.Basic",
        "widgetbrowser.pages.Misc"
      ]
    }
  },
  "requires": {
    "@qooxdoo/framework": "^6.0.0",
    "@qooxdoo/compiler": "^1.0.0"
  }
}